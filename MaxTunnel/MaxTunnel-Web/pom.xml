<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <parent>
        <artifactId>MaxTunnel-Lib</artifactId>
        <groupId>com.bandweaver</groupId>
        <version>1.0</version>
        <relativePath>../MaxTunnel-Lib/pom.xml</relativePath>
    </parent>
    <modelVersion>4.0.0</modelVersion>
    <artifactId>MaxTunnel-Web</artifactId>
    <packaging>war</packaging>
    <name>MaxTunnel-Web Maven Webapp</name>
    <url>http://maven.apache.org</url>

    <dependencies>
        <dependency>
            <groupId>junit</groupId>
            <artifactId>junit</artifactId>
            <version>3.8.1</version>
            <scope>test</scope>
        </dependency>
        <dependency>
            <groupId>com.bandweaver</groupId>
            <artifactId>MaxTunnel-Dao</artifactId>
            <version>1.0</version>
        </dependency>
        <dependency>
            <groupId>com.bandweaver</groupId>
            <artifactId>MaxTunnel-Service-Normal</artifactId>
            <version>1.0</version>
        </dependency>
        <dependency>
            <groupId>com.bandweaver</groupId>
            <artifactId>MaxTunnel-Common-Biz</artifactId>
            <version>1.0</version>
        </dependency>
    </dependencies>
    <build>
        <finalName>MaxTunnel-Web</finalName>
        <plugins>
            <!--MyBatis Generator逆向工程-->
            <plugin>
                <groupId>org.mybatis.generator</groupId>
                <artifactId>mybatis-generator-maven-plugin</artifactId>
                <version>1.3.2</version>
                <configuration>
                    <verbose>true</verbose>
                    <overwrite>true</overwrite>
                </configuration>
            </plugin>
        </plugins>
    </build>
</project>
