        select * from t_common_tunnel
        <where>
            <if test="id != null">
                and = #{id}
            </if>
            <if test="name != null and name.trim() != ''">
                and instr(name,#{name})>0
            </if>
            <if test="sn != null and sn.trim() != ''">
